{
  "name": "projetss6-tower",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build": "npx swc src -d dist",
    "clean": "rm -f $(find . -name \\*~)",
    "eslint": "npx eslint src tst",
    "check": "npx tsc -p tsconfig.json --noEmit",
    "main": "node dist/main.js",
    "parcel": "npx parcel src/main.ts",
    "test": "npx jest --coverage",
    "watchTest": "npx jest --watchAll",
    "watch": "npx tsc -p tsconfig.json --watch",
    "serve:reload": "npm run build && node --experimental-specifier-resolution=node dist/main.js",
    "dev": "tsc-watch -p tsconfig.json --onSuccess \"npm run serve:reload\""
  },
  "keywords": [],
  "author": "Khalifa Samuel, Martinez Jean-Baptiste, Chancrin Youri, Descomps Th√©o",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "commander": "^10.0.0",
    "jest": "^29.5.0",
    "parcel": "^2.8.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@swc/core": "^1.3.44",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  }
}
